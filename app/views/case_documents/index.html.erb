<p id="notice"><%= notice %></p>

<h1>Listing Case Documents</h1>

<table>
  <thead>
    <tr>
      <th>Case</th>
      <th>Document</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @case_documents.each do |case_document| %>
      <tr>
        <td><%= case_document.case_id %></td>
        <td><%= case_document.document_id %></td>
        <td><%= link_to 'Show', case_document %></td>
        <td><%= link_to 'Edit', edit_case_document_path(case_document) %></td>
        <td><%= link_to 'Destroy', case_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Case document', new_case_document_path %>
